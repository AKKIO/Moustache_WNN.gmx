hspd = keyboard_check(ord("D"))-keyboard_check(ord("A"));
vspd = keyboard_check(ord("S"))-keyboard_check(ord("W"));

//horizontal movement
if(!place_free(x+hspd*spd, y))
{
    while(place_free(x + sign(hspd*spd), y))
    {
        x += sign(hspd*spd);
    }
    hspd = 0;
}
x += hspd*spd;

//vertical movement
if(!place_free(x, y+vspd*spd))
{
    while(place_free(x, y+sign(vspd*spd)))
    {
        y += sign(vspd*spd);
    }
    vspd = 0;
}
y += vspd*spd;

if keyboard_check(vk_shift){
    spd = 4;
}
if !keyboard_check(vk_shift){
    spd = 2;
}
